prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@

Name: @PROJECT_NAME@
Description: A C++17 library to parse TMX maps generated by Tiled Map Editor.
Version: @TINYTMX_VERSION@

Libs: -L${libdir} -ltinytmx
Cflags: -I${includedir}